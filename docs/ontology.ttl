                                                           rdfs:domain <http://resource.hutime.org/ontology/UncertainTimeInterval> ;
                                                           rdfs:range <http://www.w3.org/2006/time#TemporalEntity> ;
                                                           rdfs:comment "Begnning point of a possible time interval in an uncerain time interval." ;
