@prefix : <http://plod.info/rdf/CIRO.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://plod.info/rdf/CIRO.owl#> .

<http://plod.info/rdf/CIRO.owl> rdf:type owl:Ontology ;
                                 rdfs:comment "This ontology expresses infection risks of COVID-19, toward automated assessment of infection risks of individuals."@en ;
                                 rdfs:label "COVID-19 Infection Risk Ontology (CIRO)"@en .

#################################################################
#    Object Properties
#################################################################

###  http://plod.info/rdf/action
<http://plod.info/rdf/action> rdf:type owl:ObjectProperty ;
                              rdfs:domain <http://schema.org/Event> ;
                              rdfs:range <http://plod.info/rdf/Action> .


###  http://plod.info/rdf/afford
<http://plod.info/rdf/afford> rdf:type owl:ObjectProperty ;
                              rdfs:domain <http://schema.org/Place> ;
                              rdfs:range <http://plod.info/rdf/Action> .


###  http://plod.info/rdf/age
<http://plod.info/rdf/age> rdf:type owl:ObjectProperty ;
                           rdfs:domain <http://schema.org/Person> ;
                           rdfs:range <http://plod.info/rdf/Age> .


###  http://plod.info/rdf/agent
<http://plod.info/rdf/agent> rdf:type owl:ObjectProperty ;
                             owl:inverseOf <http://plod.info/rdf/event> ;
                             rdfs:domain <http://schema.org/Event> ;
                             rdfs:range [ rdf:type owl:Class ;
                                          owl:unionOf ( <http://schema.org/Organization>
                                                        <http://schema.org/Person>
                                                      )
                                        ] .


###  http://plod.info/rdf/city
<http://plod.info/rdf/city> rdf:type owl:ObjectProperty ;
                            rdfs:domain <http://schema.org/Place> .


###  http://plod.info/rdf/context
<http://plod.info/rdf/context> rdf:type owl:ObjectProperty ;
                               rdfs:domain [ rdf:type owl:Class ;
                                             owl:unionOf ( <http://plod.info/rdf/Situation>
                                                           <http://schema.org/Event>
                                                         )
                                           ] ;
                               rdfs:range <http://plod.info/rdf/Context> .


###  http://plod.info/rdf/event
<http://plod.info/rdf/event> rdf:type owl:ObjectProperty ;
                             rdfs:domain [ rdf:type owl:Class ;
                                           owl:unionOf ( <http://schema.org/Organization>
                                                         <http://schema.org/Person>
                                                       )
                                         ] ;
                             rdfs:range <http://schema.org/Event> .


###  http://plod.info/rdf/followingEvent
<http://plod.info/rdf/followingEvent> rdf:type owl:ObjectProperty ;
                                      owl:inverseOf <http://plod.info/rdf/precedingEvent> ;
                                      rdfs:domain <http://schema.org/Event> ;
                                      rdfs:range <http://schema.org/Event> .


###  http://plod.info/rdf/isSituationOf
<http://plod.info/rdf/isSituationOf> rdf:type owl:ObjectProperty ;
                                     rdfs:domain <http://plod.info/rdf/Situation> ;
                                     rdfs:range <http://schema.org/Place> .


###  http://plod.info/rdf/precedingEvent
<http://plod.info/rdf/precedingEvent> rdf:type owl:ObjectProperty ;
                                      rdfs:domain <http://schema.org/Event> ;
                                      rdfs:range <http://schema.org/Event> .


###  http://plod.info/rdf/situation
<http://plod.info/rdf/situation> rdf:type owl:ObjectProperty ;
                                 rdfs:domain <http://schema.org/Place> ;
                                 rdfs:range <http://plod.info/rdf/Situation> .


###  http://plod.info/rdf/situationOfAction
<http://plod.info/rdf/situationOfAction> rdf:type owl:ObjectProperty ;
                                         rdfs:subPropertyOf <http://plod.info/rdf/situation> ;
                                         rdfs:range <http://plod.info/rdf/BehavioralRiskContext> .


###  http://plod.info/rdf/situationOfSpace
<http://plod.info/rdf/situationOfSpace> rdf:type owl:ObjectProperty ;
                                        rdfs:subPropertyOf <http://plod.info/rdf/situation> ;
                                        rdfs:range <http://plod.info/rdf/SpatialRiskContext> .


###  http://plod.info/rdf/time
<http://plod.info/rdf/time> rdf:type owl:ObjectProperty ;
                            rdfs:domain [ rdf:type owl:Class ;
                                          owl:unionOf ( <http://plod.info/rdf/Situation>
                                                        <http://schema.org/Event>
                                                      )
                                        ] ;
                            rdfs:range <http://plod.info/rdf/Time> .


