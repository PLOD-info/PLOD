###  http://plod.info/rdf/HighLevelCloseContact
plod:HighLevelCloseContact rdf:type owl:Class ;
                           owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                        owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                        owl:onProperty schema:location ;
                                                                                        owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                                             owl:onProperty plod:afford ;
                                                                                                             owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                                                                             owl:onClass plod:DropletReachableActivity
                                                                                                           ]
                                                                                      ]
                                                                                      [ rdf:type owl:Restriction ;
                                                                                        owl:onProperty plod:action ;
                                                                                        owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                                                        owl:onClass plod:DropletReachableActivity
                                                                                      ]
                                                                                    )
                                                                      ]
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty plod:time ;
                                                                        owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty time:hasDuration ;
                                                                                             owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                                                  owl:onProperty time:numericDuration ;
                                                                                                                  owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                                                       owl:onDatatype xsd:integer ;
                                                                                                                                       owl:withRestrictions ( [ xsd:minExclusive 15
                                                                                                                                                              ]
                                                                                                                                                            )
                                                                                                                                     ]
                                                                                                                ]
                                                                                           ]
                                                                      ]
                                                                      [ rdf:type owl:Restriction ;
                                                                        owl:onProperty plod:situationOfActivity ;
                                                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                        owl:onClass plod:RiskActivitySituation
                                                                      ]
                                                                    ) ;
                                                 rdf:type owl:Class
                                               ] ;
                           rdfs:subClassOf plod:MediumLevelCloseContact ;
                           rdfs:label "High-level Close-contact"@en ,
                                      "高密接"@ja .


